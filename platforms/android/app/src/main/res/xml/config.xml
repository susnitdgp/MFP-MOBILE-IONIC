    <name>PRADIP</name>
    <description>Mobile app for Office Note Process and Dashboards</description>
    <author email="ecm@ntpc.co.in" href="https://pradip.ntpc.co.in/">NTPC EOC (IT)</author>
    <content src="index.html" />
